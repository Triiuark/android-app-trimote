<resources>
	<declare-styleable name="SeekBarPreference">
		<attr name="withDefault" format="boolean" />
		<attr name="minValue" format="integer" />
		<attr name="maxValue" format="integer" />
		<attr name="defaultString" format="string" />
		<attr name="prefix" format="string" />
		<attr name="suffix" format="string" />
	</declare-styleable>
	<declare-styleable name="InodePreference">
		<attr name="needsWrite" format="boolean" />
		<attr name="type">
			<!-- has to be InodePreference.TYPE_FILE -->
			<flag name="file" value="0x01" />
			<!-- has to be InodePreference.TYPE_DIR  -->
			<flag name="directory" value="0x02" />
		</attr>
	</declare-styleable>
</resources>
